{
    "name": "nickurt/laravel-postcodeapi",
    "description": "Universal PostcodeApi for Laravel 9.x & 10.x",
    "keywords": [
        "postcodeapi",
        "postcode",
        "api",
        "universal",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.15",
        "orchestra/testbench": "^6.6|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "nickurt\\PostcodeApi\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "nickurt\\PostcodeApi\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "nickurt\\PostcodeApi\\ServiceProvider"
            ],
            "aliases": {
                "PostcodeApi": "nickurt\\PostcodeApi\\Facade"
            }
        }
    }
}
